{
  // "manifest_version": 2,
  "name": "Split Search",
  "version": "1.0",
  "description": "Splittttt.",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.co.jp/*",
        "http://www.google.co.jp/*"
      ],
      "js": ["commonWindow.js", "mainWindow.js", "frameWindow.js", "contentscript.js", "injectedscript.js"],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ]
}

